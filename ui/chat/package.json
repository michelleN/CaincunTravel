{
    "name": "cancun-chat",
    "version": "1.0",
    "description": "a travel agency that is totally normal and fine",
    "main": "src/index.tsx",
    "dependencies": {
        "bookshelf-deep-changed": "1.0.0",
        "deep-chat-react": "latest",
        "react": "18.0.0",
        "react-dom": "18.0.0",
        "react-scripts": "4.0.3",
        "@tailwindcss/cli": "^4.0.3",
        "deep-chat-react": "^2.1.1",
        "nodemon": "^3.1.9",
        "sass": "^1.83.4"
    },
    "devDependencies": {
        "@types/react": "18.0.25",
        "@types/react-dom": "18.0.9",
        "typescript": "4.4.2",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.5.1",
        "tailwindcss": "^4.0.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
